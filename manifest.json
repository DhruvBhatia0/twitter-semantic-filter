{
    "manifest_version": 3,
    "name": "Website and Post Blocker",
    "version": "1.0.0",
    "description": "Blocks certain websites and specific posts on x.com",
    "permissions": [
      "webNavigation",
      "tabs"
    ],
    "host_permissions": [
      "*://*.instagram.com/*",
      "*://*.youtube.com/*",
      "*://*.x.com/*"
    ],
    "background": {
      "service_worker": "background.js"
    }
  }